{
  "name": "@saasfly/db",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": "./index.ts",
    "./prisma/types": "./prisma/types.ts",
    "./prisma/enums": "./prisma/enums.ts"
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prisma format && prettier --check '**/*.{ts,json}' ",
    "db:generate": "prisma generate",
    "db:push": "npm run with-env -- prisma db push --skip-generate",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@auth/kysely-adapter": "^1.11.1",
    "@react-email/components": "^0.5.7",
    "@vercel/postgres-kysely": "^0.10.0",
    "kysely": "0.27.3",
    "next": "^14.2.33",
    "next-auth": "^4.24.12",
    "nitropack": "^2.12.9"
  },
  "devDependencies": {
    "@saasfly/eslint-config": "0.0.1",
    "@saasfly/prettier-config": "0.1.0",
    "@saasfly/typescript-config": "0.0.1",
    "@types/pg": "8.11.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "prisma": "5.9.1",
    "prisma-kysely": "^2.2.1",
    "typescript": "5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@saasfly/eslint-config/base"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  },
  "prettier": "@saasfly/prettier-config"
}

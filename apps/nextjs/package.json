{
  "name": "@saasfly/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -c ./tailwind.config.js -i ./src/app/globals.css -o ./src/app/tw.css --minify",
    "build:db": "cd ../../packages/db && bun run db:generate",
    "build": "npm run build:db && npm run build:css && next build",
    "dev": "set NODE_OPTIONS=--dns-result-order=ipv4first && next dev",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.20.0",
    "next-auth": "4.24.7",
    "next": "14.2.25",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "lucide-react": "^0.379.0",
    "framer-motion": "^11.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.25",
    "postcss": "^8.4.38",
    "typescript": "5.4.5"
  }
}